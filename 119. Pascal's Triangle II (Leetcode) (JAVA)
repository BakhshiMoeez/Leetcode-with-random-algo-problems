class Solution {
    public List<Integer> getRow(int rowIndex) {
        List<List<Integer>> pascalTraingle = new ArrayList<>();
        List<Integer> temp = new ArrayList<>();
        if(rowIndex == 0) {
            List<Integer> temp2 = new ArrayList<>();
            temp2.add(1);
            return temp2;
        }
        for(int i = 0; i <= rowIndex; i++) {
            List<Integer> row = new ArrayList<>();
            for(int j = 0; j <= i; j++) {
                if(j == 0 || j == i) {
                    row.add(1);
                } else {
                    temp = pascalTraingle.get(i-1);
                    row.add(temp.get(j-1)+temp.get(j));
                }
            }
            pascalTraingle.add(row);
        }

        return pascalTraingle.get(pascalTraingle.size()-1);
    }
}

void moveZeroes(vector<int>& nums) 
    {
        int i = 0, j = 0;
        while(j < nums.size())
        {
            if(j < nums.size() && nums[j] != 0)
            {
                nums[i] = nums[j];
                i++;
                j++;
            }
            
            while(j < nums.size() && nums[j] == 0)
            {
                j++;
            }
        }

        if(i == 0)
        {
            return;
        }

        while(i < nums.size())
        {
            nums[i] = 0;
            i++;
        }            
    }
